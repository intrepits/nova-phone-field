{
    "name": "intrepits/nova-phone-field",
    "description": "A phone field for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Intrepits\\NovaPhoneField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Intrepits\\NovaPhoneField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
